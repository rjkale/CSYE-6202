        <TextBox x:Name="txtBoxFirstName" HorizontalAlignment="Left" Height="26" Margin="181,87,-31,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="142" AcceptsTab="True" IsEnabled="False"/>
